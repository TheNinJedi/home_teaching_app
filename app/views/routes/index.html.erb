<html>
<head>
  <meta charset="UTF-8">
  <title>Routes</title>
</head>
<body>
  
  <div class="row text-center">
    <h1>Routes in this District</h1>
  </div>
  <div>
    <h4 class="col-xs-12 text-left">Route-ID</h4>
  </div>
  <div>
    <% @routes.each do |route| %>
    <ul class="col-xs-12 text-left">
      <li>
        <%# route.id %>
        <%= link_to "#{route.id}", route_path(route.id) %>
      </li>
    </ul>
    <% end %>
  </div>
  

</body>
</html>